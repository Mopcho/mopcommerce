// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       String @id @unique @db.Uuid
  username String
  email    String @unique
  password String

  @@map("users")
}

model Items {
  id    String @id @unique @db.Uuid
  title String

  @@map("items")
}
